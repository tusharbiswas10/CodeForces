#include <bits/stdc++.h>
#define ll long long
#define pb push_back
#define max2(a,b) ((a<b)?b:a)
#define max3(a,b,c) max2(max2(a,b),c)
#define min2(a,b) ((a>b)?b:a)
#define min3(a,b,c) min2(min2(a,b),c)



using namespace std;





int main(){

   int t;
   cin>>t;
    for(ll i=0;i<t;i++){

      ll a,b;
      cin>>a>>b;

      cout<<a*b<<"\n";
     
       
    }
    
    return 0;
}
