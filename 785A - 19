#include <iostream>

using namespace std;

int
main ()
{
  int a, b,e=0;
  string c;
  cin >> a;

  for (int i = 0; i < a; i++)
    {
      cin >> c;

      if (c == "Tetrahedron")
	{
	  b = 4;
	}
      else if (c == "Cube")
	{
	  b = 6;
	}
      else if (c == "Octahedron")
	{
	  b = 8;
	}
      else if (c == "Dodecahedron")
	{
	  b = 12;
	}
      else if (c == "Icosahedron")
	{
      b=20;
	}
	
	e+=b;
	
	
	
    }
    
    cout<<e;
  return 0;
}
