#include <bits/stdc++.h>
#include <iostream>
#define ll long long int
#define mod 1000000007
#define endl        "\n"
#define pb          push_back
#define max2(a,b)   ((a<b)?b:a)
#define max3(a,b,c) max2(max2(a,b),c)
#define min2(a,b)   ((a>b)?b:a)
#define min3(a,b,c) min2(min2(a,b),c)
 
 
using namespace std;


 

 
 
 
 
int main(){
    
   
   ios_base::sync_with_stdio(false); cin.tie(NULL);

   ll a,b,c;

    cin>>a>>b;
     if(a==b){
     	cout<<a<<endl;
     }

     else{

     	if(a==0 & b==1){
     		cout<<2<<endl;
     	}

     	if(a==1 & b==0){
     		cout<<2<<endl;
     	}

     	if(a==0 & b==2){
     		cout<<1<<endl;
     	}

     	if(a==2 & b==0){
     		cout<<1<<endl;
     	}

     	if(a==1 & b==2){
     		cout<<0<<endl;
     	}
     	if(a==2 & b==1){
     		cout<<0<<endl;
     	}

     }
 	
 	
    

    
    return 0;
}
